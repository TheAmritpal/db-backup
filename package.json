{
  "name": "db-backup",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx --bun astro dev",
    "start": "bunx --bun astro dev",
    "build": "astro check && bunx --bun astro build",
    "preview": "bun run ./dist/server/entry.mjs",
    "astro": "astro",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate --schema=./src/db/schema/index.ts --out=./src/db/migrations --dialect=mysql",
    "db:migrate": "bun run --bun ./src/db/migrate.ts"
  },
  "dependencies": {
    "@astrojs/check": "0.9.5",
    "@elysiajs/cron": "^1.4.1",
    "@elysiajs/eden": "^1.4.4",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@nurodev/astro-bun": "^2.0.5",
    "astro": "5.15.2",
    "drizzle-orm": "^0.44.7",
    "drizzle-typebox": "^0.3.3",
    "elysia": "^1.4.13",
    "googleapis": "^164.1.0",
    "lucia": "^3.2.2",
    "mysql2": "^3.15.3",
    "sweetalert2": "^11.26.3",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "^1.3.1",
    "drizzle-kit": "^0.31.6"
  },
  "override": {
    "@sinclair/typebox": "0.32.4"
  }
}
