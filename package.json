{
  "name": "db-backup",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "bun run ./dist/server/entry.mjs",
    "astro": "astro",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate --schema=./src/db/schema/index.ts --out=./src/db/migrations --dialect=mysql",
    "db:migrate": "bun run --bun ./src/db/migrate.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@elysiajs/eden": "^1.1.2",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "astro": "^4.14.3",
    "astro-bun-adapter": "^1.0.2",
    "bcrypt": "^5.1.1",
    "drizzle-orm": "^0.33.0",
    "elysia": "^1.1.7",
    "lucia": "^3.2.0",
    "mysql2": "^3.11.0",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "^1.1.6",
    "drizzle-kit": "^0.24.0"
  }
}
